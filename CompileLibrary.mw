<Group labelreference="L256751" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">Describe(LULEM):</Text-field>
</Input>
<Output><Text-field style="Line Printed Output" layout="Line Printed Output">
# LU Decomposition for Large Expression Matrices
module LULEM:

    SetModuleOptions::symbol = LULEM:-SetModuleOptions

    # Veil an expression &lt;x&gt; and return a label to it.
    Veil( x, $ )

    # UnVeil the expression &lt;x&gt; up to &lt;n&gt; levels.
    UnVeil( x, n::{infinity, nonnegint}, $ )

    # Show the veiling variables of the veiling label &lt;label&gt;.
    ShowVeil( label::{symbol}, $ )

    # Return a list of the veiling variables labelled as &lt;label&gt;.
    ListVeil( label::{symbol}, $ )

    # Substitute the reversed veiling variables of the veiling label &lt;label&gt; in 
    # the expression &lt;x&gt;.
    SubsVeil( label::{symbol}, x, $ )

    # Clear all the veiling variables of the veiling label &lt;label&gt;.
    ForgetVeil( label::{symbol}, $ )

    # Assign the list &lt;x&gt; to the local variable &lt;StoredData&gt;.
    AssignData( x::{list, set}, $ )

    # Substitute the local variable &lt;StoredData&gt; in the expression &lt;x&gt;.
    SubsData( x::{anything}, $ )

    # Unassign the local variable &lt;StoredData&gt;.
    UnAssignData( $ )

    # Compute the LU decomposition premutation matrix provided the NAG-style 
    # the pivot vector &lt;r&gt;.
    PermutationMatrix( r::{Vector}, $ )

    # Compute the LU decomposition of a square matrix &lt;A&gt; using the LEM 
    # strategy &lt;Strategy_Veiling&gt;, the veiling symbol &lt;Q&gt;, the pivoting 
    # strategy &lt;Strategy_Pivots&gt; and the zero recognition strategy 
    # &lt;Strategy_Zero&gt;.
    LUD( A::{Matrix}, Q::{symbol}, 
         Strategy_Veiling::{procedure} := _VeilingStrategy_n, 
         Strategy_Pivots::{procedure} := _PivotStrategy_Slength, 
         Strategy_Zero::{procedure} := _ZeroStrategy_length, $ )

    # Compute the LU decomposition of a square matrix &lt;A&gt; using the LEM 
    # strategy &lt;Strategy_Veiling&gt;, the veiling symbol &lt;Q&gt;, the pivoting 
    # strategy &lt;Strategy_Pivots&gt; and the zero recognition strategy 
    # &lt;Strategy_Zero&gt;.
    FFLUD( A::{Matrix}, Q::{symbol}, 
           Strategy_Veiling::{procedure} := _VeilingStrategy_n, 
           Strategy_Pivots::{procedure} := _PivotStrategy_Slength, 
           Strategy_Zero::{procedure} := _ZeroStrategy_length, $ )

    # Solve the linear system Ax=b using LULEM algorithm, privided the matrix 
    # &lt;A&gt;, the vector &lt;b&gt;, the veiling symbol &lt;Q&gt;, the veiling strategy 
    # &lt;Strategy_Veiling&gt;, the pivoting strategy &lt;Strategy_Pivots&gt; and the zero 
    # recognition strategy &lt;Strategy_Zero&gt;.
    Solve( A::{Matrix}, b::{Vector}, Q::{symbol, function}, 
           Strategy_Veiling::{procedure} := _VeilingStrategy_n, 
           Strategy_Pivots::{procedure} := _PivotStrategy_Slength, 
           Strategy_Zero::{procedure} := _ZeroStrategy_length, $ )

    # Veiling strategy: number of indeterminates in expression &lt;x&gt; minus 4.
    VeilingStrategy_n( x, $ ) :: {integer}

    # Veiling strategy: length of expression &lt;x&gt; minus 50.
    VeilingStrategy_L( x, $ ) :: {integer}

    # Veiling strategy: length of expression &lt;x&gt; minus 120.
    VeilingStrategy_Ls( x, $ ) :: {integer}

    # Veiling strategy: length of expression &lt;x&gt; minus 260.
    VeilingStrategy_LB( x, $ ) :: {integer}

    # Pivoting strategy: choose the pivot with the largest length between 
    # expressions &lt;x&gt; and &lt;y&gt;.
    PivotStrategy_Llength( x::{algebraic}, y::{algebraic}, $ ) :: {boolean}

    # Pivoting strategy: choose the pivot with the smallest length between 
    # expressions &lt;x&gt; and &lt;y&gt;.
    PivotStrategy_Slength( x::{algebraic}, y::{algebraic}, $ ) :: {boolean}

    # Pivoting strategy: choose the pivot with the largest number of 
    # indeterminates between expressions &lt;x&gt; and &lt;y&gt;.
    PivotStrategy_Lindets( x::{algebraic}, y::{algebraic}, $ ) :: {boolean}

    # Pivoting strategy: choose the pivot with the smallest number of 
    # indeterminates between expressions &lt;x&gt; and &lt;y&gt;.
    PivotStrategy_Sindets( x::{algebraic}, y::{algebraic}, $ ) :: {boolean}

    # Pivoting strategy: choose the pivot with the largest numeric value 
    # between expressions &lt;x&gt; and &lt;y&gt;.
    PivotStrategy_numeric( x::{algebraic}, y::{algebraic}, $ ) :: {boolean}

    # Zero recognition strategy: length of expression &lt;x&gt;.
    ZeroStrategy_length( x::{algebraic}, $ ) :: {integer}

    # Zero recognition strategy: normalizer of expression &lt;x&gt;.
    ZeroStrategy_normalizer( x::{algebraic}, $ )

    package LastUsed:

